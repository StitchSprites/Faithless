{
  "model": {
    "type": "select",
    "property": "display_context",
    "cases": [
      {
        "when": [ "gui", "fixed" ],
        "model": {
          "type": "select",
          "property": "trim_material",
          "cases": [
            { "when": "amethyst", 	"model": { "type": "model", "model": "trim/netherite/leggings_amethyst" } },
            { "when": "copper", 	"model": { "type": "model", "model": "trim/netherite/leggings_copper" } },
            { "when": "diamond", 	"model": { "type": "model", "model": "trim/netherite/leggings_diamond" } },
            { "when": "emerald", 	"model": { "type": "model", "model": "trim/netherite/leggings_emerald" } },
            { "when": "gold", 		"model": { "type": "model", "model": "trim/netherite/leggings_gold" } },
            { "when": "iron", 		"model": { "type": "model", "model": "trim/netherite/leggings_iron" } },
            { "when": "lapis", 		"model": { "type": "model", "model": "trim/netherite/leggings_lapis" } },
            { "when": "netherite", 	"model": { "type": "model", "model": "trim/netherite/leggings_netherite" } },
            { "when": "quartz", 	"model": { "type": "model", "model": "trim/netherite/leggings_quartz" } },
            { "when": "redstone", 	"model": { "type": "model", "model": "trim/netherite/leggings_redstone" } },
            { "when": "resin", 		"model": { "type": "model", "model": "trim/netherite/leggings_resin" } }
          ],
          "fallback": {
            "type": "model",
            "model": "item/netherite_leggings"
          }
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "item/netherite_leggings_alt"
    }
  }
}