{
	"model": {
		"type": "select",
		"property": "component",
		"component": "axolotl/variant",
		"cases": [
			{ "when": "lucy", "model": { "type": "model", "model": "item/axolotl_bucket" } },
			{ "when": "wild", "model": { "type": "model", "model": "item/axolotl_wild" } },
			{ "when": "gold", "model": { "type": "model", "model": "item/axolotl_gold" } },
			{ "when": "cyan", "model": { "type": "model", "model": "item/axolotl_cyan" } },
			{ "when": "blue", "model": { "type": "model", "model": "item/axolotl_blue" } }
		],
		"fallback": { "type": "model", "model": "item/axolotl_bucket" }
	}
}